	<content><![CDATA[
ll powermod(ll b,ll e,ll m)
{
	if(e==0) return 1;
	if(e&1) return b*powermod(b*b%m,e/2,m)%m;
	return powermod(b*b%m,e/2,m);
}
]]></content>
